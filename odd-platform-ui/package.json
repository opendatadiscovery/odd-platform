{
  "name": "odd-platform-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "build-only": "vite build",
    "clean": "rm -rf ./src/generated-sources",
    "build": "pnpm generate && tsc --noEmit && vite build",
    "test": "vitest",
    "tsc": "tsc",
    "generate": "docker run -v `pwd`/..:/mnt -w /mnt --rm openapitools/openapi-generator-cli:v5.1.1 generate -c odd-platform-ui/openapi-config.yaml",
    "lint": "eslint --ext .tsx,.ts src/",
    "lint:fix": "eslint --ext .tsx,.ts src/ --fix",
    "prepare": "chmod u+x .husky/prepare.sh && .husky/prepare.sh",
    "pre-commit": "pnpm tsc --noEmit && lint-staged",
    "preinstall": "pnpm install husky && npx only-allow pnpm"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint -c .eslintrc.json --fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@hookform/error-message": "^2.0.1",
    "@mui/lab": "^5.0.0-alpha.128",
    "@mui/material": "^5.12.2",
    "@mui/system": "^5.12.1",
    "@mui/x-date-pickers": "^5.0.20",
    "@reduxjs/toolkit": "^1.8.5",
    "@tanstack/react-query": "^4.29.12",
    "@tanstack/react-virtual": "^3.0.0-beta.39",
    "@uiw/react-md-editor": "^3.19.0",
    "@visx/curve": "^3.0.0",
    "@visx/event": "^3.0.1",
    "@visx/group": "^3.0.0",
    "@visx/marker": "^3.0.0",
    "@visx/shape": "^3.0.0",
    "@visx/text": "^3.0.0",
    "@visx/zoom": "^3.1.1",
    "ajv": "^8.11.0",
    "d3-hierarchy": "^3.1.1",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.1.0",
    "d3-zoom": "^3.0.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "elkjs": "^0.8.2",
    "handlebars": "^4.7.7",
    "html-react-parser": "^3.0.8",
    "jotai": "^2.0.3",
    "lodash": "^4.17.21",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-hot-toast": "^2.4.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-multi-date-picker": "^3.3.4",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.9.0",
    "react-truncate-markup": "^5.1.2",
    "styled-components": "^5.3.10",
    "use-debounce": "^8.0.1",
    "uuid": "^9.0.0",
    "vanilla-jsoneditor": "^0.7.11"
  },
  "devDependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.8.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/d3": "^7.4.0",
    "@types/d3-hierarchy": "^3.1.2",
    "@types/d3-selection": "^3.0.5",
    "@types/d3-shape": "^3.1.1",
    "@types/d3-zoom": "^3.0.2",
    "@types/hast": "^2.3.4",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.13",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-redux": "^7.1.11",
    "@types/styled-components": "^5.1.26",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/unist": "^2.0.6",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.8.4",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vite-plugin-checker": "^0.5.6",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.29.8"
  },
  "resolutions": {
    "minimist": "1.2.6"
  },
  "engines": {
    "node": "v18.16.0",
    "pnpm": "^8.4.0"
  }
}
